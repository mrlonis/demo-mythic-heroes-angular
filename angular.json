{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "cf0b9a86-d966-4336-aeea-010a6bb353ad",
    "defaultCollection": "@angular-eslint/schematics"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "demo-mythic-heroes-angular": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/demo-mythic-heroes-angular",
      "sourceRoot": "projects/demo-mythic-heroes-angular/src",
      "prefix": "mrlonis",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo-mythic-heroes-angular",
            "index": "projects/demo-mythic-heroes-angular/src/index.html",
            "main": "projects/demo-mythic-heroes-angular/src/main.ts",
            "polyfills": "projects/demo-mythic-heroes-angular/src/polyfills.ts",
            "tsConfig": "projects/demo-mythic-heroes-angular/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/demo-mythic-heroes-angular/src/favicon.ico",
              "projects/demo-mythic-heroes-angular/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "projects/demo-mythic-heroes-angular/src/styles/styles.scss",
              "./node_modules/bootstrap/dist/css/bootstrap.min.css"
            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.min.js",
              "./node_modules/bootstrap/dist/js/bootstrap.min.js",
              "./node_modules/popper.js/dist/umd/popper.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/demo-mythic-heroes-angular/src/environments/environment.ts",
                  "with": "projects/demo-mythic-heroes-angular/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "demo-mythic-heroes-angular:build:production"
            },
            "development": {
              "browserTarget": "demo-mythic-heroes-angular:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-mythic-heroes-angular:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/demo-mythic-heroes-angular/src/test.ts",
            "polyfills": "projects/demo-mythic-heroes-angular/src/polyfills.ts",
            "tsConfig": "projects/demo-mythic-heroes-angular/tsconfig.spec.json",
            "karmaConfig": "projects/demo-mythic-heroes-angular/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/demo-mythic-heroes-angular/src/favicon.ico",
              "projects/demo-mythic-heroes-angular/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "projects/demo-mythic-heroes-angular/src/styles/styles.scss",
              "./node_modules/bootstrap/dist/css/bootstrap.min.css"
            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.min.js",
              "./node_modules/bootstrap/dist/js/bootstrap.min.js",
              "./node_modules/popper.js/dist/umd/popper.min.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/demo-mythic-heroes-angular/**/*.ts",
              "projects/demo-mythic-heroes-angular/**/*.html"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "demo-mythic-heroes-angular"
}
