    "prettier": "^2.5.1",
    "puppeteer": "^12.0.1",
    "typescript": "~4.4.4",
    "webpack": "^5.65.0",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "lint-staged": {
    "*.{js,scss,html,css,md,json}": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write",
      "ng-lint-staged lint --fix --"
    ]
