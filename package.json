{
  "name": "demo-mythic-heroes-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "prettier": "prettier --config .prettierrc --write \"projects/**/*.{ts,js,css,scss,html,json}\"",
    "test": "ng test",
    "test:ci": "npm run test -- --watch=false --code-coverage --reporters progress,coverage,junit --browsers ChromeHeadless",
    "lint": "ng lint demo-mythic-heroes-angular --fix",
    "format:write": "prettier projects/**/*.{ts,json,scss,html} --write",
    "format:test": "prettier projects/**/*.{ts,json,scss,html} --list-different",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/demo-mythic-heroes-angular/stats.json",
    "prepare": "husky install",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.0.3",
    "@angular/cdk": "^13.0.4",
