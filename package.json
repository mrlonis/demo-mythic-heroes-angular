{
  "name": "demo-mythic-heroes-angular",
  "version": "0.0.0",
  "scripts": {
    "husky:init": "npx husky-init",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-production": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/demo-mythic-heroes-angular/stats.json",
    "test": "ng test",
    "test:ci": "npm run test -- --watch=false --code-coverage --reporters progress,coverage,junit --browsers ChromeHeadless",
    "lint": "ng lint demo-mythic-heroes-angular",
    "lint:fix": "ng lint demo-mythic-heroes-angular --fix",
    "eslint:fix": "eslint -c ./.eslintrc.json --fix --ext .ts,.html .",
    "prettier": "prettier --config .prettierrc --write \"./**/*.{ts,js,css,scss,html,json,md}\"",
    "prettier:write": "prettier ./**/*.{ts,json,scss,html} --write",
    "prettier:test": "prettier ./**/*.{ts,json,scss,html} --list-different",
    "build-storybook": "ng run demo-mythic-heroes-angular:build-storybook",
    "storybook": "ng run demo-mythic-heroes-angular:storybook",
    "lint-staged": "lint-staged"
